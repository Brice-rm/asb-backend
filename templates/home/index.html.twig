{% extends 'base.html.twig' %}



{% block body %}
<!--  Carroussel Accueil-->
<section class="carroussel">
    <div id="carousel" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#carousel" data-slide-to="0" class="active"></li>
            <li data-target="#carousel" data-slide-to="1"></li>
            <li data-target="#carousel" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img class="d-block w-100" src="img/conception.jpg" alt="First slide">
            </div>
            <div class="carousel-item">
                <img class="d-block w-100" src="img/energies.jpg" alt="Second slide">
            </div>
            <div class="carousel-item">
                <img class="d-block w-100" src="img/renovation.jpg" alt="Third slide">
            </div>
        </div>
        <a class="carousel-control-prev" href="#carousel" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carousel" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</section>
<!--  End Carroussel Accueil-->
<!--  Présentation-->
<section class="presentation">
    <h1>A.S.B. elec - Votre artisan électricien</h1>
    <hr>
    <p>Lorem ipsum, dolor sit amet consectetur adipisicing elit. Sed ut recusandae quis cupiditate provident
        exercitationem magni eveniet sint facilis, harum voluptatibus praesentium sunt impedit, atque unde saepe dolorum
        reprehenderit ea quae quidem non. Eaque exercitationem incidunt quidem inventore ex accusantium, quo expedita
        nisi impedit eius dolorem dolorum, placeat dolor qui.</p>
    <a href="#who" class="btn-accueil">Qui sommes-nous?</a>
</section>
<!--  End Présentation -->
<!--  Services -->




<section class="services" id="services">
    <h2>Services</h2>
    <hr>    
    <div class="carousel-wrap">
        <div class="owl-carousel">
            {% for service in services %}
            <div class="container">
                <img src="/img/{{ service.image }}">
                <div class="block-text overed">
                     <h3><!--Energies renouvelables--> {{ service.titre }}</h3> 
                    <hr class="hr-services">
                    <p><!--  - Collecte d'information <br>
                        - Etude des données <br>
                        - Bilan de votre installation existante-->
                        {{ service.description }}
                    </p> 
                </div>
            </div>
            {% endfor %}
        </div>
    </div>
</section>

<!--  End Services -->
<!--  Métier -->
<section class="metier" id="metiers">
    <div class="bloc-1">
        <div class="bloc-txt">
            <h3>Notre métier</h3>
            <p>Lorem ipsum dolor sit amet consectetur adipisicing elit. Doloremque ea nisi ullam, placeat delectus
                laborum nihil blanditiis quos. Repudiandae eaque laboriosam asperiores ipsum iste cum, atque sint omnis
                ipsam nulla ratione. Minus, dolor odio dolores quis aut saepe asperiores impedit perspiciatis molestiae,
                beatae omnis fugiat necessitatibus ad quam culpa doloremque?</p>
            <button class="btn-bloc" type="button">Contactez-nous</button>
        </div>
        <img src="img/autre.jpg" alt="tableau électrique">
    </div>
</section>
<!--  End Métier -->
<!--  Réalisations -->
<section class="realisation" id="realisations">
    <h2>Réalisations</h2>
    <hr>
    <div class="carousel-wrap">
        <div class="owl-carousel">
            {% for realisation in realisations %}
            <div class="container">
                <img src="/img/{{ realisation.image }}">
                <div class="block-text opacity">
                    <h3>{{ realisation.titre }}</h3>
                    <p>{{ realisation.Soustitre }}</p>
                </div>
            </div>
            {% endfor %}
        </div>
    </div>
</section>
<!--  End Réalisations -->
<!--  Qui sommes-nous? -->
<section class="qui-sommes-nous" id="who">
    <h2>Qui sommes-nous ?</h2>
    <hr>
    <div class="bloc-2">
        <div class="bloc-2-1">
            <div class="bloc-txt-1">
                <h3>Notre métier</h3>
                <p>Lorem ipsum dolor sit amet consectetur adipisicing elit. Doloremque ea nisi ullam, placeat delectus
                    laborum nihil blanditiis quos. Repudiandae eaque laboriosam asperiores ipsum iste cum, atque sint
                    omnis ipsam nulla ratione. Minus, dolor odio dolores quis aut saepe asperiores impedit perspiciatis
                    molestiae, beatae omnis fugiat necessitatibus ad quam culpa doloremque?</p>
                <button class="btn-bloc" type="button">Contactez-nous</button>
            </div>
            <img src="img/autre.jpg" alt="tableau électrique">
        </div>
        <div class="bloc-2-2">
            <img src="img/autre.jpg" alt="tableau électrique">
            <div class="bloc-txt-2">
                <h3>Notre engagement</h3>
                <p>Vous proposer un devis gratuit <br>
                <ul>
                    <li>Respect des délais</li>
                    <li>Soins des finitions</li>
                    <li>Utilisation d'un matériel certifié aux normes</li>
                    <li>Intervention en urgence 7j/7 de 08h00 à 19h00</li>
                    <li>Conseils et solutions adaptées à votre projet</li>
                </ul>
                </p>
                <button class="btn-bloc" type="button">Contactez-nous</button>
            </div>
        </div>
    </div>
</section>
<!--  End Qui sommes-nous? -->
<!--  Fonctionnement -->
<section class="fonctionnement" id="fonctionnements">
    <h2>Fonctionnement</h2>
    <hr>
    <div class="carousel-wrap">
        <div class="owl-carousel1">
            <div class="container">
                <img src="img/home/formulaire.jpg">
                <div class="block-text-fonctionnement">
                    <h3>1 - Contactez-nous</h3>
                    <hr class="hr-fonction">
                    <p>Expliquez-nous votre projet via notre formulaire de <br> contact en ligne ou contactez-nous
                        directement par mail <br> a.s.b.elec@gmail.com <br>
                        ou par téléphone au <br>
                        06 08 66 18 12.</p>
                </div>
            </div>
            <div class="container">
                <img src="img/home/devis.jpg">
                <div class="block-text-fonctionnement">
                    <h3>2 - Devis</h3>
                    <hr class="hr-fonction">
                    <p>Nous nous engageons à vous apporter une réponse dans un délais de 48 heures. <br>
                        Un rendez-vous sera rapidement fixé pour établir un devis chiffré.</p>
                </div>
            </div>
            <div class="container">
                <img src="img/home/travaux.jpg">
                <div class="block-text-fonctionnement">
                    <h3>3 - Travaux</h3>
                    <hr class="hr-fonction">
                    <p>Lorem ipsum dolor, sit amet consectetur adipisicing elit. Eum ex, corporis iure cumque enim,
                        quibusdam harum reprehenderit adipisci non ducimus amet dolores sapiente, eaque excepturi?</p>
                </div>
            </div>
        </div>
    </div>
</section>
<!--  End Fonctionnment -->
<!--  Contact -->
<section class="contact" id="contacts">
    <h2>Contact</h2>
    <hr>
    <div class="contact-form">
        <input type="text" name="name" id="name" required placeholder="Votre nom">
        <input type="text" name="firstname" id="firstname" required placeholder="Votre prénom">
        <input type="phone" name="phone" id="phone" required placeholder="Votre téléphone">
        <input type="email" name="email" id="email" required placeholder="Votre mail">
        <input type="textarea" name="name" id="description" required placeholder="Votre message">
        <input type="button" value="Envoyez" id="submit">
    </div>
</section>
<!--  End Contacts -->
<!--  Infos -->
<section class="infos">
    <h2>Infos & accès</h2>
    <hr>
    <div class="all-infos">
        <div class="infos-hour">
            <h3>Horaires d'ouverture</h3>
            <hr>
            <p>Lundi : 09h00 - 19h00</p>
            <p>Mardi : 09h00 - 19h00</p>
            <p>Mercredi : 09h00 - 19h00</p>
            <p>Jeudi : 09h00 - 19h00</p>
            <p>Vendredi : 09h00 - 19h00</p>
        </div>
        <div class="access">
            <h3>Localisez-nous</h3>
            <hr>
            <iframe
                src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2577.8418067409616!2d4.69386082655046!3d49.75142025269314!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47ea11dc9a2414f3%3A0xaad954ba2e838dae!2s47%20Route%20de%20Warnecourt%2C%2008000%20Prix-l%C3%A8s-M%C3%A9zi%C3%A8res!5e0!3m2!1sfr!2sfr!4v1617690830264!5m2!1sfr!2sfr"
                width="250" height="250" style="border:0;" allowfullscreen="" loading="lazy"></iframe>
        </div>
    </div>
    <div class="asb-infos">
        <h3>A.S.B Elec</h3>
        <hr class="asb">
        <p>MAZZOLINI Benoît</p>
        <p>47 Route de Warnecourt</p>
        <p>08000 Prix-les-Mézières</p>
        <p>06 08 66 18 12</p>
        <p>a.s.b.elec@gmail.com</p>
    </div>
</section>

<!--  End Infos -->
{% endblock %}